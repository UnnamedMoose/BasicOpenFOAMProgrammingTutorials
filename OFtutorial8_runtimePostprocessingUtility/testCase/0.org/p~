FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "../system/runConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 101300;

boundaryField
{
    inlet
    {
        type        zeroGradient;
    }
    outlet
    {
        type		fixedValue;
		value		$internalField;
    }
	pipe
    {
    	type zeroGradient;
    }
    "symmetry.*"
    {
        type            symmetryPlane;
    }
}

