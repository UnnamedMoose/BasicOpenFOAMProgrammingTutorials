FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
	inlet
	{
		type            fixedValue;
		value           uniform (1 0 0);
	}

    walls
    {
        type            noSlip;
    }

	outlet
	{
		type 			zeroGradient;
	}

    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
